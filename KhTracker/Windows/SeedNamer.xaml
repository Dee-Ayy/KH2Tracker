<Window x:Class="KhTracker.SeedNamer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KhTracker"
        mc:Ignorable="d"
        Title="Change Seed Name" Height="150" Width="300"
        ResizeMode="CanMinimize"
        Background="#303030">
    <StackPanel Margin="10">
        <Label Content="Enter Seed Name" HorizontalAlignment="Center" Foreground="White"/>
        <TextBox x:Name="inputTextBox" Width="133" HorizontalAlignment="Center"/>
        <Button Content="Generate Card" IsDefault="True" Click="OkButton_Click" Margin="0,10,0,0" Padding="10,2" HorizontalAlignment="Center"/>
    </StackPanel>
</Window>
