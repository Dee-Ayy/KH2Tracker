<Window x:Class="KhTracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KhTracker"
        mc:Ignorable="d"
        ResizeMode="CanResize"
        KeyDown="Window_KeyDown"
        Closing="Window_Closing"
        LocationChanged="Window_LocationChanged"
        SizeChanged="Window_SizeChanged"
        AllowDrop="True" Drop="DropFile"
        Icon="/Images/Other/ansem_report.png" Title="KH2Tracker 2.0 (Alpha)" Height="880" Width="570" Background="#303030"
        MinHeight="440" MinWidth="300">

    <Window.Resources>
        <Style TargetType="{x:Type Image}">
            <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality"/>
        </Style>
    </Window.Resources>

    <DockPanel>
        <Menu DockPanel.Dock="Top" Background="White">
            <Menu.ItemsPanel>
                <ItemsPanelTemplate>
                    <DockPanel HorizontalAlignment="Stretch"/>
                </ItemsPanelTemplate>
            </Menu.ItemsPanel>
            <MenuItem Header="Options" Focusable="False">
                <MenuItem Header="Auto Tracking...">
                    <MenuItem Header="Use Auto-Detect Game Version" Click="AutoDetectToggle" x:Name="AutoDetectOption" IsCheckable="True" IsChecked="False"/>
                    <MenuItem Header="Start PCSX2 Tracking" 	    Click="InitPCSX2Tracker"/>
                    <MenuItem Header="Start PC Port Tracking" 	    Click="InitPCTracker"/>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Load Hints...">
                    <MenuItem Header="Load JSmartee Hints (*.hints)" 	Click="LoadHints"/>
                    <MenuItem Header="Load Shan Hints (*.pnach)" 		Click="ParseSeed"/>
                    <MenuItem Header="Load KH2Randomizer Seed (*.zip)" 	Click="OpenKHSeed"/>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Save Progress" Click="SaveProgress"/>
                <MenuItem Header="Load Progress" Click="LoadProgress"/>
                <Separator/>
                <MenuItem Header="Open Broadcast Window" 		Click="BroadcastWindow_Open" />
                <MenuItem Header="Broadcast Window On Startup" 	Click="BroadcastStartupToggle" IsCheckable="True" IsChecked="False" x:Name="BroadcastStartupOption"/>
                <Separator/>
                <MenuItem Header="Reset Window Size" Click="ResetSize"/>
                <MenuItem Header="Reset Tracker" 	 Click="OnReset"/>
                <MenuItem Header="Always on Top" 	 Click="TopMostToggle"  x:Name="TopMostOption"     IsCheckable="True" IsChecked="False"/>
                <MenuItem Header="Drag and Drop" 	 Click="DragDropToggle" x:Name="DragAndDropOption" IsCheckable="True" IsChecked="True"/>
            </MenuItem>
            <MenuItem Header="Toggles" Focusable="False">
                <MenuItem Header="Ansem Reports" 			 Click="ReportsToggle"		x:Name="ReportsOption"      IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Promise Charm"			 Click="PromiseCharmToggle" x:Name="PromiseCharmOption" IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Once More / Second Chance" Click="AbilitiesToggle" 	x:Name="AbilitiesOption"    IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Anti Form" 				 Click="AntiFormToggle"		x:Name="AntiFormOption"     IsCheckable="True" IsChecked="False"/>
                <MenuItem Header="World Lock Checks"         Click="VisitLockToggle"	x:Name="VisitLockOption"    IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Extra Important Checks" 	 Click="ExtraChecksToggle"	x:Name="ExtraChecksOption"  IsCheckable="True" IsChecked="True"/>
                <Separator/>
                <MenuItem Header="Show Seed Hash on Load"				  Click="SeedHashToggle"		x:Name="SeedHashOption"			IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Show World Progression Icons" 		  Click="WorldProgressToggle"	x:Name="WorldProgressOption"	IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Show Form and Growth Levels" 			  Click="FormsGrowthToggle"		x:Name="FormsGrowthOption"		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Show Growth Levels on Broadcast Window" Click="BroadcastGrowthToggle"	x:Name="BroadcastGrowthOption" 	IsCheckable="True" IsChecked="False"/>
                <MenuItem Header="Show Sora Stats on Broadcast Window" 	  Click="BroadcastStatsToggle"	x:Name="BroadcastStatsOption" 	IsCheckable="True" IsChecked="False"/>
                <Separator/>
                <MenuItem Header="Track Ghost Items (Points Mode)" 	  Click="GhostItemToggle"		x:Name="GhostItemOption"	IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Ghost Item Auto-Math (Points Mode)" Click="GhostMathToggle" 		x:Name="GhostMathOption"	IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Show Check Count (Points Mode)"     Click="ShowCheckCountToggle"  x:Name="CheckCountOption"	IsCheckable="True" IsChecked="False"/>
                <Separator/>
                <MenuItem Header="Show Next Level Check Display" Click="NextLevelCheckToggle" x:Name="NextLevelCheckOption" IsCheckable="True" IsChecked="False"/>
                <MenuItem Header="Level 1 Checks"                Click="SoraLevel01Toggle"    x:Name="SoraLevel01Option"    IsCheckable="True" IsChecked="False"/>
                <MenuItem Header="Level 50 Checks"               Click="SoraLevel50Toggle"    x:Name="SoraLevel50Option"    IsCheckable="True" IsChecked="True" />
                <MenuItem Header="Level 99 Checks"               Click="SoraLevel99Toggle"    x:Name="SoraLevel99Option"    IsCheckable="True" IsChecked="False"/>
                <MenuItem Header="Show Death Counter" 	 		 Click="DeathCounterToggle"   x:Name="DeathCounterOption"   IsCheckable="True" IsChecked="False"/>
            </MenuItem>
            <MenuItem Header="Visuals" Focusable="False">
                <!-- eventually add an ption that pulls up settings window... -->
                <!--MenuItem Header="Simple Stat/UI Icons" Click="MinStatToggle" x:Name="MinStatOption" IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Game Stat/UI Icons"   Click="OldStatToggle" x:Name="OldStatOption" IsCheckable="True" IsChecked="False"/>
                <Separator/-->
                <MenuItem Header="Simple World Icons" Click="MinWorldToggle" x:Name="MinWorldOption" IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Game World Icons"   Click="OldWorldToggle" x:Name="OldWorldOption" IsCheckable="True" IsChecked="False"/>
                <Separator/>
                <MenuItem Header="Simple Check Icons" Click="MinCheckToggle" x:Name="MinCheckOption" IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Game Check Icons"   Click="OldCheckToggle" x:Name="OldCheckOption" IsCheckable="True" IsChecked="False"/>
                <Separator/>
                <MenuItem Header="Simple Progression Icons"	Click="MinProgToggle" x:Name="MinProgOption" IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Game Progression Icons" 	Click="OldProgToggle" x:Name="OldProgOption" IsCheckable="True" IsChecked="False"/>
                <!--Separator/>
                <MenuItem x:Name="MinNumOption" Header="Kh2 Number Icons" IsCheckable="True" IsChecked="True"  Click="MinNumToggle"/>
                <MenuItem x:Name="OldNumOption" Header="Classic Number Icons"	 IsCheckable="True" IsChecked="False" Click="OldNumToggle"/-->
                <Separator/>
                <MenuItem Header="Load Custom Images" Click="CustomImageToggle" x:Name="CustomFolderOption" IsCheckable="True" IsChecked="False"/>
                <Separator/>
                <MenuItem Header="Main Window Background">
                    <MenuItem x:Name="MainDefOption" Header="Default" IsCheckable="True" IsChecked="True"  Click="MainBG_DefToggle"/>
                    <MenuItem x:Name="MainImg1Option" Header="Custom Image (Stretch)"    IsCheckable="True" IsChecked="False" Click="MainBG_Img1Toggle"/>
                    <MenuItem x:Name="MainImg2Option" Header="Custom Image (Uniform)"    IsCheckable="True" IsChecked="False" Click="MainBG_Img2Toggle"/>
                    <MenuItem x:Name="MainImg3Option" Header="Custom Image (No Scaling)" IsCheckable="True" IsChecked="False" Click="MainBG_Img3Toggle"/>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Broadcast Window Background">
                    <MenuItem x:Name="BroadcastDefOption" Header="Default" IsCheckable="True" IsChecked="True"  Click="BroadcastBG_DefToggle"/>
                    <MenuItem x:Name="BroadcastImg1Option" Header="Custom Image (Stretch)"    IsCheckable="True" IsChecked="False" Click="BroadcastBG_Img1Toggle"/>
                    <MenuItem x:Name="BroadcastImg2Option" Header="Custom Image (Uniform)"    IsCheckable="True" IsChecked="False" Click="BroadcastBG_Img2Toggle"/>
                    <MenuItem x:Name="BroadcastImg3Option" Header="Custom Image (No Scaling)" IsCheckable="True" IsChecked="False" Click="BroadcastBG_Img3Toggle"/>
                </MenuItem>
            </MenuItem>
            <MenuItem Header="Worlds" Focusable="False">
                <MenuItem Header="Sora's Heart" 				Click="SoraHeartToggle"			x:Name="SoraHeartOption"		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Drive Forms" 					Click="DrivesToggle"			x:Name="DrivesOption" 			IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Simulated Twilight Town" 		Click="SimulatedToggle" 		x:Name="SimulatedOption"		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Twilight Town"  				Click="TwilightTownToggle" 		x:Name="TwilightTownOption"		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Hollow Bastion" 				Click="HollowBastionToggle" 	x:Name="HollowBastionOption"	IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Beast's Castle" 				Click="BeastCastleToggle" 		x:Name="BeastCastleOption" 		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Olympus Coliseum" 			Click="OlympusToggle" 			x:Name="OlympusOption" 			IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Agrabah" 						Click="AgrabahToggle" 			x:Name="AgrabahOption" 			IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Land of Dragons" 				Click="LandofDragonsToggle" 	x:Name="LandofDragonsOption"	IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Disney Castle" 				Click="DisneyCastleToggle" 		x:Name="DisneyCastleOption"		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Pride Lands" 					Click="PrideLandsToggle" 		x:Name="PrideLandsOption" 		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Port Royal" 					Click="PortRoyalToggle" 		x:Name="PortRoyalOption"		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Halloween Town" 				Click="HalloweenTownToggle" 	x:Name="HalloweenTownOption"	IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Space Paranoids" 				Click="SpaceParanoidsToggle" 	x:Name="SpaceParanoidsOption" 	IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="The World that Never Was" 	Click="TWTNWToggle" 			x:Name="TWTNWOption"			IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="100 Acre Wood" 				Click="HundredAcreWoodToggle" 	x:Name="HundredAcreWoodOption" 	IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Atlantica" 					Click="AtlanticaToggle" 		x:Name="AtlanticaOption"		IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Synthesis" 					Click="SynthToggle" 			x:Name="SynthOption"			IsCheckable="True" IsChecked="True"/>
                <MenuItem Header="Puzzles" 						Click="PuzzleToggle" 			x:Name="PuzzleOption"			IsCheckable="True" IsChecked="True"/>
            </MenuItem>
            <MenuItem x:Name="ModeDisplay" Header=" " Focusable="False" IsHitTestVisible="False"/>
            <!--MenuItem Header="-DA 1.6.8-" IsHitTestVisible="False" Focusable="False" HorizontalAlignment="Right"/-->
        </Menu>

        <Grid>
            <Grid.RowDefinitions>
                <!-- 6.6 -->
                <RowDefinition x:Name="WorldRow" Height="6.6*"/>
                <!-- 0.8 -->
                <RowDefinition x:Name="InfoRow"  Height="0.8*"/>
                <!-- 0.5 -->
                <RowDefinition x:Name="FormRow"  Height="0.5*"/>
                <!-- 2.2 -->
                <RowDefinition x:Name="CheckRow" Height="2.2*"/>
                <!-- 0.4 -->
                <RowDefinition x:Name="SettingRow" Height="0.0*"/>
            </Grid.RowDefinitions>

            <!-- World Grids -->
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>

                <!-- Left Side -->
                <Grid Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>

                    <!-- Sora's Heart -->
                    <Grid x:Name="SorasHeartTop" Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition Height="0.04*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.27*"/>
                            <!-- Path Width 0.08 -->
                            <ColumnDefinition Width="0.0*"/>
                            <ColumnDefinition Width="0.73*"/>
                        </Grid.ColumnDefinitions>
                        <!-- Main Icons -->
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="0.5*"/>
                                <ColumnDefinition Width="0.5*"/>
                            </Grid.ColumnDefinitions>
                            <!-- main icon -->
                            <Button Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name ="SorasHeart" Background="Transparent" PreviewMouseDown="OnMouseDown" MouseWheel="OnMouseWheel" Template="{StaticResource DefaultButton}" FocusVisualStyle="{x:Null}" Content="{DynamicResource Min-SoraHeartImage}"/>
                            <!-- cross icon -->
                            <Image Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name="SorasHeartCross" Source="Images/Other/crossworld.png" Visibility="Collapsed" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5" Opacity="0.9">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2"/>
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- world value -->
                            <Viewbox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Panel.ZIndex="1" HorizontalAlignment="Right" Margin="4,0,4,0">
                                <local:OutlinedTextBlock x:Name="SorasHeartHint" FontFamily="/KhTracker;component/Fonts/#KHGummi" FontSize="20pt" StrokeThickness="5" Stroke="Black" Fill="#FFEFECE9" Text="?"/>
                            </Viewbox>
                            <!-- progression -->
                            <ContentControl x:Name="SorasHeartType" Panel.ZIndex="1" IsHitTestVisible="False" Grid.Column="2" Grid.ColumnSpan="2" HorizontalAlignment="Left"/>
                        </Grid>
                        <!-- path hints icons -->
                        <Grid x:Name="SorasHeartPath" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.18*"/>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition/>
                                <RowDefinition Height="0.18*"/>
                            </Grid.RowDefinitions>
                            <Image x:Name="SorasHeartPath_Con" Source="Images/Checks/Simple/proof_of_connection.png"      Visibility="Visible" Grid.Row="1" VerticalAlignment="Bottom"/>
                            <Image x:Name="SorasHeartPath_Non" Source="Images/Checks/Simple/proof_of_nonexistence.png"    Visibility="Visible" Grid.Row="2"/>
                            <Image x:Name="SorasHeartPath_Pea" Source="Images/Checks/Simple/proof_of_tranquility.png"     Visibility="Visible" Grid.Row="3" VerticalAlignment="Top"/>
                        </Grid>
                        <!-- item grid -->
                        <local:WorldGrid x:Name="SorasHeartGrid" Grid.Column="2"/>
                        <!-- fancy colors -->
                        <Rectangle Grid.ColumnSpan="2" Fill="#BF1E1E1E" Panel.ZIndex="-1"/>
                        <Rectangle Grid.Row="1" Grid.Column="2" Fill="#BF1E1E1E" Panel.ZIndex="-1"/>
                    </Grid>

                    <!-- Simulated Twilight Town -->
                    <Grid x:Name="SimulatedTwilightTownTop" Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition Height="0.04*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.27*"/>
                            <!-- Path Width 0.08 -->
                            <ColumnDefinition Width="0.0*"/>
                            <ColumnDefinition Width="0.73*"/>
                        </Grid.ColumnDefinitions>
                        <!-- Main Icons -->
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="0.5*"/>
                                <ColumnDefinition Width="0.5*"/>
                            </Grid.ColumnDefinitions>
                            <!-- main icon -->
                            <Button Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name ="SimulatedTwilightTown" Background="Transparent" PreviewMouseDown="OnMouseDown" MouseWheel="OnMouseWheel" Template="{StaticResource DefaultButton}" FocusVisualStyle="{x:Null}" Content="{DynamicResource Min-SimulatedImage}"/>
                            <!-- cross icon -->
                            <Image Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name="SimulatedTwilightTownCross" Source="Images/Other/crossworld.png" Visibility="Collapsed" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5" Opacity="0.9">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2"/>
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- world value -->
                            <Viewbox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Panel.ZIndex="1" HorizontalAlignment="Right" Margin="4,0,4,0">
                                <local:OutlinedTextBlock x:Name="SimulatedTwilightTownHint" FontFamily="/KhTracker;component/Fonts/#KHGummi" FontSize="20pt" StrokeThickness="5" Stroke="Black" Fill="#FFEFECE9" Text="?"/>
                            </Viewbox>
                            <!-- progression -->
                            <ContentControl x:Name="SimulatedTwilightTownProgression" Panel.ZIndex="1" IsHitTestVisible="False" Grid.Column="2" Grid.ColumnSpan="2" HorizontalAlignment="Left"/>
                        </Grid>
                        <!-- path hints icons -->
                        <Grid x:Name="SimulatedTwilightTownPath" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.18*"/>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition/>
                                <RowDefinition Height="0.18*"/>
                            </Grid.RowDefinitions>
                            <Image x:Name="SimulatedTwilightTownPath_Con" Source="Images/Checks/Simple/proof_of_connection.png"      Visibility="Visible" Grid.Row="1" VerticalAlignment="Bottom"/>
                            <Image x:Name="SimulatedTwilightTownPath_Non" Source="Images/Checks/Simple/proof_of_nonexistence.png"    Visibility="Visible" Grid.Row="2"/>
                            <Image x:Name="SimulatedTwilightTownPath_Pea" Source="Images/Checks/Simple/proof_of_tranquility.png"     Visibility="Visible" Grid.Row="3" VerticalAlignment="Top"/>
                        </Grid>
                        <!-- item grid -->
                        <local:WorldGrid x:Name="SimulatedTwilightTownGrid" Grid.Column="2"/>
                        <!-- fancy spacer colors -->
                        <Rectangle Grid.ColumnSpan="2" Fill="#BF1E1E1E" Panel.ZIndex="-1"/>
                        <Rectangle Grid.Row="1" Grid.Column="2" Fill="#BF1E1E1E" Panel.ZIndex="-1"/>
                    </Grid>

                    <!-- Hollow Bastion -->
                    <Grid x:Name="HollowBastionTop" Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition Height="0.04*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.27*"/>
                            <!-- Path Width 0.08 -->
                            <ColumnDefinition Width="0.0*"/>
                            <ColumnDefinition Width="0.73*"/>
                        </Grid.ColumnDefinitions>
                        <!-- Main Icons -->
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="0.5*"/>
                                <ColumnDefinition Width="0.5*"/>
                            </Grid.ColumnDefinitions>
                            <!-- main icon -->
                            <Button Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name ="HollowBastion" Background="Transparent" PreviewMouseDown="OnMouseDown" MouseWheel="OnMouseWheel" Template="{StaticResource DefaultButton}" FocusVisualStyle="{x:Null}" Content="{DynamicResource Min-HollowBastionImage}"/>
                            <!-- cross icon -->
                            <Image Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name="HollowBastionCross" Source="Images/Other/crossworld.png" Visibility="Collapsed" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5" Opacity="0.9">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2"/>
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- lock icon -->
                            <Image x:Name="HollowBastionLock" Source="Images/Other/visitlock.png" Grid.RowSpan="2" Grid.ColumnSpan="3" Panel.ZIndex="1" RenderTransformOrigin="0.5,0.5" IsHitTestVisible="False">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="0.5" ScaleY="0.5"/>
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- world value -->
                            <Viewbox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Panel.ZIndex="1" HorizontalAlignment="Right" Margin="4,0,4,0">
                                <local:OutlinedTextBlock x:Name="HollowBastionHint" FontFamily="/KhTracker;component/Fonts/#KHGummi" FontSize="20pt" StrokeThickness="5" Stroke="Black" Fill="#FFEFECE9" Text="?"/>
                            </Viewbox>
                            <!-- progression -->
                            <ContentControl x:Name="HollowBastionProgression" Panel.ZIndex="1" IsHitTestVisible="False" Grid.Column="2" Grid.ColumnSpan="2" HorizontalAlignment="Left"/>
                        </Grid>
                        <!-- path hints icons -->
                        <Grid x:Name="HollowBastionPath" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.18*"/>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition/>
                                <RowDefinition Height="0.18*"/>
                            </Grid.RowDefinitions>
                            <Image x:Name="HollowBastionPath_Con" Source="Images/Checks/Simple/proof_of_connection.png"      Visibility="Visible" Grid.Row="1" VerticalAlignment="Bottom"/>
                            <Image x:Name="HollowBastionPath_Non" Source="Images/Checks/Simple/proof_of_nonexistence.png"    Visibility="Visible" Grid.Row="2"/>
                            <Image x:Name="HollowBastionPath_Pea" Source="Images/Checks/Simple/proof_of_tranquility.png"     Visibility="Visible" Grid.Row="3" VerticalAlignment="Top"/>
                        </Grid>
                        <!-- item grid -->
                        <local:WorldGrid x:Name="HollowBastionGrid" Grid.Column="2"/>
                        <!-- fancy spacer colors -->
                        <Rectangle Grid.ColumnSpan="2" Fill="#BF1E1E1E" Panel.ZIndex="-1"/>
                        <Rectangle Grid.Row="1" Grid.Column="2" Fill="#BF1E1E1E" Panel.ZIndex="-1"/>
                    </Grid>

                    <!-- Olympus Coliseum -->
                    <Grid x:Name="OlympusColiseumTop" Grid.Row="3">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition Height="0.04*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.27*"/>
                            <!-- Path Width 0.08 -->
                            <ColumnDefinition Width="0.0*"/>
                            <ColumnDefinition Width="0.73*"/>
                        </Grid.ColumnDefinitions>
                        <!-- Main Icons -->
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="0.5*"/>
                                <ColumnDefinition Width="0.5*"/>
                            </Grid.ColumnDefinitions>
                            <!-- main icon -->
                            <Button Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name ="OlympusColiseum" Background="Transparent" PreviewMouseDown="OnMouseDown" MouseWheel="OnMouseWheel" Template="{StaticResource DefaultButton}" FocusVisualStyle="{x:Null}" Content="{DynamicResource Min-OlympusImage}"/>
                            <!-- cross icon -->
                            <Image Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name="OlympusColiseumCross" Source="Images/Other/crossworld.png" Visibility="Collapsed" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5" Opacity="0.9">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.2" ScaleY="1.2"/>
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- lock icon -->
                            <Image x:Name="OlympusColiseumLock" Source="Images/Other/visitlock.png" Grid.RowSpan="2" Grid.ColumnSpan="3" Panel.ZIndex="1" RenderTransformOrigin="0.5,0.5" IsHitTestVisible="False">
                                <Image.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="0.5" ScaleY="0.5"/>
                                    </TransformGroup>
                                </Image.RenderTransform>
                            </Image>
                            <!-- world value -->
                            <Viewbox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Panel.ZIndex="1" HorizontalAlignment="Right" Margin="4,0,4,0">
                                <local:OutlinedTextBlock x:Name="OlympusColiseumHint" FontFamily="/KhTracker;component/Fonts/#KHGummi" FontSize="20pt" StrokeThickness="5" Stroke="Black" Fill="#FFEFECE9" Text="?"/>
                            </Viewbox>
                            <!-- progression -->
                            <ContentControl x:Name="OlympusColiseumProgression" Panel.ZIndex="1" IsHitTestVisible="False" Grid.Column="2" Grid.ColumnSpan="2" HorizontalAlignment="Left"/>
                        </Grid>
                        <!-- path hints icons -->
                        <Grid x:Name="OlympusColiseumPath" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.18*"/>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition/>
